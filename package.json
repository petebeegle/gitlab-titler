{
  "name": "gitlab-titler",
  "version": "1.0.0",
  "description": "Append JIRA tickets to gitlab merge requests",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "rome check src/*",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petebeegle/gitlab-titler.git"
  },
  "bugs": {
    "url": "https://github.com/petebeegle/gitlab-titler/issues"
  },
  "homepage": "https://github.com/petebeegle/gitlab-titler#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "jest": "^29.4.1",
    "rome": "^11.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.mjsx?$": "babel-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).mjs?(x)"
    ]
  }
}
